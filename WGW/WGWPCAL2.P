            pmuwd132.uom_v  = n_uom_v
            pmuwd132.trndat = TODAY
            pmuwd132.trntim = STRING(TIME,"HH:MM:SS")
            pmuwd132.usrid  = USERID(LDBNAME(1))
            pmuwd132.effdat = nv_effdat

        nv_prem_p0 = nv_prem2.
       RUN wgw\wgwomx00s (INPUT nv_tariff,           /* DRIVER'S AGE */
                              nv_drivcod,
                              nv_class,
                              nv_covcod,
                              nv_key_b,
                              nv_comdat,
               INPUT-OUTPUT   nv_prem1,
               INPUT-OUTPUT   nv_prem2).   /*Kridtiya  I.  A51-0198*/
       nv_drivprm  = nv_prem1 - nv_prvprm.
       nv_prvprm   = nv_prem1.
    END.
    /*chk
    message color blue/white "DRIV" nv_drivprm nv_prem1.
    pause .
    chk*/
